{
  "expo": {
    "name": "Crown Space",
    "slug": "afro-hair-service",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "afro-hair-service",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/logo/crownLogoIos.png",
    "ios": {
      "icon": "./assets/logo/crownLogoIos.png",
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.afrohairservice.app",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "CFBundleLocalizations": [
          "en"
        ]
      }
    },
    "android": {
      "icon": "./assets/logo/crownLogoAndroid.png",
      "adaptiveIcon": {
        "backgroundColor": "#330670"
      },
      "package": "com.afrohairservice.app",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.afrohairservice.app",
          "enableGooglePay": true
        }
      ],
      "expo-apple-authentication",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access use your photo library."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#330670",
          "image": "./assets/logo/crownLogoSplashScreen.png",
          "imageWidth": 200
        }
      ],
      "patch-project",
      "@react-native-google-signin/google-signin"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "9fc86b35-0796-4e35-a9c6-bdbb8056c9b9"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9fc86b35-0796-4e35-a9c6-bdbb8056c9b9"
    }
  }
}
